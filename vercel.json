{
  "version": 2,
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "env": {
    "REACT_APP_EXCHANGE_RATE_API_KEY": "@exchange_rate_api_key",
    "REACT_APP_EXCHANGE_RATE_BASE_URL": "@exchange_rate_base_url"
  }
}